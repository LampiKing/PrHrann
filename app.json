{
  "expo": {
    "name": "PrHran",
    "slug": "prhran",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon-512.png",
    "scheme": "prhran",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./public/icon-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#10b981"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prhran.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/icon-512.png",
        "backgroundColor": "#10b981"
      },
      "package": "com.prhran.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./public/icon-192.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
