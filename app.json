{
  "expo": {
    "name": "PrHran",
    "slug": "prhran",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon-512.png",
    "scheme": "prhran",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./public/icon-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#10b981"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prhran.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "PrHran potrebuje dostop do kamere za skeniranje računov in črtnih kod.",
        "NSPhotoLibraryUsageDescription": "PrHran potrebuje dostop do galerije za nalaganje slik računov.",
        "NSLocationWhenInUseUsageDescription": "PrHran lahko uporabi vašo lokacijo za prikaz najbližjih trgovin.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/icon-512.png",
        "backgroundColor": "#10b981"
      },
      "package": "com.prhran.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "blockedPermissions": [
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./public/icon-192.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "TVOJ_EAS_PROJECT_ID"
      }
    },
    "owner": "lampipizza",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/TVOJ_EAS_PROJECT_ID"
    }
  }
}
